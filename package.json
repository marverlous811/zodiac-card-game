{
  "name": "card-game",
  "version": "1.0.0",
  "description": "seed project build for typescript",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.config.js --open",
    "test": "mocha dist/test/app.test.js --timeout 10000000",
    "server": "node dist/app.js",
    "server-build": "tsc -w",
    "build": "webpack --config webpack-prod.config.js",
    "clean": "rm -rf ./dist/*",
    "api-test": "SVR_HOST=\"http://localhost:8080\" mocha dist/test/server/server.test.js",
    "start-server": "npm run clean && tsc && node dist/app.js",
    "start-client": "npm run clean && npm run start",
    "start-test": "npm run clean && tsc && npm run test"
  },
  "author": "marverlous",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.11",
    "chai": "^4.2.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/node": "^12.0.0",
    "await-to-js": "^2.1.1",
    "fastify": "^2.3.0",
    "socket.io": "^2.2.0"
  }
}
